				 0,
				 true); //check for overlaps
  G4VPhysicalVolume* fPhysAraWindowLat12 = new G4PVPlacement(0,G4ThreeVector((fCryostat_x/2-fthickness/4)*m,(fCryostat_y/2-0.32)*m,(-fCryostat_z/2+0.75+15*1.5)*m),"ArapucaWindowLat12",
                                 fLogicAraWindowLat,     //its logical volume
                                 fPhysCryostat,    	//its mother  volume
