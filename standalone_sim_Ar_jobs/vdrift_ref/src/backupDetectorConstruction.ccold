  G4VPhysicalVolume* fPhysAraWindowLat13 = new G4PVPlacement(0,G4ThreeVector((fCryostat_x/2-fthickness/4)*m,(fCryostat_y/2-0.32)*m,(-fCryostat_z/2+0.75+16*1.5)*m),"ArapucaWindowLat13",
